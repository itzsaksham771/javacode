// code to inherit derive class
class base {
    public int x;
    public int getx() {
        return x;
    }
    public void setx(int x) {
        this.x=x;
    }
}
class derived extends base {
    public int y;
    public int gety() {
        return y;
    }
    public void sety(int y) {
        this.y=y;
    }
}
public class saksham_45_inheritance {
    public static void main(String[] args) {
        base b = new base();
        b.setx(4);
        System.out.println(b.getx());

        derived d = new derived();
        d.sety(8);
        System.out.println(d.gety());
    }
}
