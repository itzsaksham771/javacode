interface bicycle {
    int a= 45;
    void applybrake(int decrement);
    void speedUp(int increment);
}

interface HornBicycle {
    void blowhornk3g();
    void blowhornmhn();
}
class Avoncycle implements bicycle, HornBicycle {
    public void blowhorm() {
        System.out.println("Pee po po po");
    }
    public void applybrake(int decrement) {
        System.out.println("break applied ");
    }
    public void speedUp(int increment) {
        System.out.println("Speeding up");
    }
    public void blowhornk3g() {
        System.out.println("Systumm Systumm...........");
    }
    public void blowhornmhn() {
        System.out.println("Main hoon na bhai....");
    }
}
public class saksham_55_interface {
    public static void main(String[] args) {
        Avoncycle avc = new Avoncycle();
        avc.applybrake(1);
        System.out.println(avc.a);
        avc.blowhornk3g();
        avc.blowhornmhn();
    }
}
